{
  "name": "flutter_docker",
  "context": "..",
  "dockerFile": "../Dockerfile",
  "remoteUser": "developer",
  "runArgs": ["--privileged"],
  "extensions": ["dart-code.flutter"],
  "workspaceMount": "source=${localWorkspaceFolder},target=/home/developer/workspace/store_flutter,type=bind,consistency=delegated",
  "workspaceFolder": "/home/developer/workspace/store_flutter"
}
